{
  "name": "energy-cloud-frontend",
  "version": "1.1.2",
  "private": true,
  "description": "",
  "keywords": [],
  "license": "ISC",
  "author": "",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "commit": "cz",
    "dev": "pnpm --parallel --filter \"./**\" dev",
    "dev:main": "pnpm --filter \"./packages/main-project\" dev",
    "lint-staged": "lint-staged",
    "format": "prettier --config .prettierrc --check \"./**.{vue,ts,css,less,scss}\" --write",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.md,.json --max-warnings 0",
    "lint:fix": "pnpm run lint --fix",
    "ls-lint": "ls-lint",
    "standard-version": "standard-version -- --dry-run",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -w -r 0",
    "release": "node ./scripts/release.mjs"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@element-plus/eslint-config": "^0.0.20230307",
    "@ls-lint/ls-lint": "^1.11.2",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jsdom": "^21.1.0",
    "@types/node": "^18.14.6",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/tsconfig": "^0.1.3",
    "commitizen": "^4.3.0",
    "conventional-changelog": "^3.1.25",
    "conventional-changelog-cli": "^2.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.35.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "^8.0.3",
    "inquirer": "^9.1.5",
    "jsdom": "^21.1.0",
    "lint-staged": "^13.2.1",
    "ls-lint": "^0.1.2",
    "only-allow": "^1.1.1",
    "shelljs": "^0.8.5",
    "standard-version": "^9.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-plugin-eslint": "^1.8.1",
    "vue": "^3.2.47",
    "vue-tsc": "^1.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx,md,json}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "__npminstall_done": false
}
