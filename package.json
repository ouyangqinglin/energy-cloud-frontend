    "prettier": "^2.3.2",
    "pro-download": "1.0.1",
    "puppeteer-core": "^8.0.0",
    "stylelint": "^13.0.0",
    "typescript": "^4.2.2",
    "typescript-plugin-css-modules": "^5.0.2"
  },
  "resolutions": {
    "@types/react": "17.0.0",
    "@types/react-dom": "^17.0.0",
    "d3-selection": "3.0.0"
  },
  "overrides": {
    "d3-selection": "3.0.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
